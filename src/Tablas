CREATE TABLE pacientes(id INTEGER(11) NOT NULL PRIMARY KEY, nombre TEXT(100) NOT NULL, apellido TEXT (100) NOT NULL, fecha_nacimiento TEXT, telefono INTEGER(9), creado_en TEXT NOT NULL, modificado TEXT NOT NULL);
CREATE TABLE cirugias_paciente (id INTEGER PRIMARY KEY NOT NULL, cirugia TEXT NOT NULL, paciente_id INTEGER(11), FOREIGN KEY (paciente_id) REFERENCES pacientes (id));
CREATE TABLE alergias_paciente (id INTEGER PRIMARY KEY NOT NULL,alergias TEXT(200) NOT NULL,paciente_id INTEGER(11),FOREIGN KEY (paciente_id) REFERENCES pacientes (id));
CREATE TABLE consulta_paciente (id INTEGER PRIMARY KEY NOT NULL, consulta TEXT, paciente_id INTEGER(11),FOREIGN KEY (paciente_id) REFERENCES pacientes (id));
CREATE TABLE diagnostico_paciente (id INTEGER PRIMARY KEY NOT NULL, diagnostico TEXT NOT NULL, paciente_id INTEGER(11), FOREIGN KEY (paciente_id) REFERENCES pacientes (id));
CREATE TABLE ejercicio_paciente (id INTEGER PRIMARY KEY NOT NULL, ejercicio TEXT NOT NULL, paciente_id INTEGER(11), FOREIGN KEY (paciente_id) REFERENCES pacientes (id));
CREATE TABLE medicacion_paciente (id INTEGER PRIMARY KEY NOT NULL, medicacion TEXT(200) NOT NULL, paciente_id INTEGER(11), FOREIGN KEY (paciente_id) REFERENCES pacientes (id));
CREATE TABLE ocupacion_paciente (id INTEGER PRIMARY KEY NOT NULL, ocupacion TEXT(200) NOT NULL, paciente_id INTEGER(11), FOREIGN KEY (paciente_id) REFERENCES pacientes (id));
CREATE TABLE tratamiento_paciente (id INTEGER PRIMARY KEY NOT NULL, tratamiento TEXT, paciente_id INTEGER(11), FOREIGN KEY (paciente_id) REFERENCES pacientes (id));
CREATE TABLE valoracion_paciente (id INTEGER PRIMARY KEY NOT NULL, valoracion TEXT NOT NULL, paciente_id INTEGER(11), FOREIGN KEY (paciente_id) REFERENCES pacientes (id));