CREATE TABLE `alergias_paciente` (`id` int(11) NOT NULL AUTO_INCREMENT,`alergias` varchar(200) NOT NULL,`paciente_id` int(11) DEFAULT NULL,PRIMARY KEY (`id`),KEY `id_paciente` (`paciente_id`),CONSTRAINT `alergias_paciente_ibfk_1` FOREIGN KEY (`paciente_id`) REFERENCES `pacientes` (`id`));
CREATE TABLE `cirugias_paciente` (`id` int(11) NOT NULL AUTO_INCREMENT,`cirugia` text NOT NULL,`paciente_id` int(11) DEFAULT NULL,PRIMARY KEY (`id`),KEY `paciente_id` (`paciente_id`),CONSTRAINT `cirugias_paciente_ibfk_1` FOREIGN KEY (`paciente_id`) REFERENCES `pacientes` (`id`));
CREATE TABLE `pacientes` (`id` int(11) NOT NULL AUTO_INCREMENT,`nombre` varchar(100) NOT NULL,`apellido` varchar(100) NOT NULL,`fecha_nacimiento` date DEFAULT NULL,`telefono` int(9) DEFAULT NULL,`creado_en` date NOT NULL,`modificado` date NOT NULL,PRIMARY KEY (`id`));
CREATE TABLE `consulta_paciente` (`id` int(11) NOT NULL AUTO_INCREMENT,`consulta` text,`paciente_id` int(11) DEFAULT NULL,PRIMARY KEY (`id`),KEY `paciente_id` (`paciente_id`),CONSTRAINT `consulta_paciente_ibfk_1` FOREIGN KEY (`paciente_id`) REFERENCES `pacientes` (`id`));
CREATE TABLE `diagnostico_paciente` (`id` int(11) NOT NULL AUTO_INCREMENT,`diagnostico` text NOT NULL,`paciente_id` int(11) DEFAULT NULL,PRIMARY KEY (`id`),KEY `paciente_id` (`paciente_id`),CONSTRAINT `diagnostico_paciente_ibfk_1` FOREIGN KEY (`paciente_id`) REFERENCES `pacientes` (`id`));
CREATE TABLE `ejercicio_paciente` (`id` int(11) NOT NULL AUTO_INCREMENT,`ejercicio` text NOT NULL,`paciente_id` int(11) DEFAULT NULL,PRIMARY KEY (`id`),KEY `paciente_id` (`paciente_id`),CONSTRAINT `ejercicio_paciente_ibfk_1` FOREIGN KEY (`paciente_id`) REFERENCES`pacientes` (`id`));
CREATE TABLE `medicacion_paciente` (`id` int(11) NOT NULL AUTO_INCREMENT,`medicacion` text NOT NULL,`paciente_id` int(11) DEFAULT NULL,PRIMARY KEY (`id`),KEY `paciente_id` (`paciente_id`),CONSTRAINT `medicacion_paciente_ibfk_1` FOREIGN KEY (`paciente_id`) REFERENCES`pacientes` (`id`));
CREATE TABLE `ocupacion_paciente` (`id` int(11) NOT NULL AUTO_INCREMENT,`ocupacion` varchar(200) NOT NULL,`paciente_id` int(11) DEFAULT NULL,PRIMARY KEY (`id`),KEY `paciente_id` (`paciente_id`),CONSTRAINT `ocupacion_paciente_ibfk_1` FOREIGN KEY (`paciente_id`) REFERENCES`pacientes` (`id`));
CREATE TABLE `tratamiento_paciente` (`id` int(11) NOT NULL AUTO_INCREMENT,`tratamiento` text,`paciente_id` int(11) DEFAULT NULL,PRIMARY KEY (`id`),KEY `paciente_id` (`paciente_id`),CONSTRAINT `tratamiento_paciente_ibfk_1` FOREIGN KEY (`paciente_id`) REFERENCES `pacientes` (`id`));
CREATE TABLE `valoracion_paciente` (`id` int(11) NOT NULL AUTO_INCREMENT,`valoracion` text NOT NULL,`paciente_id` int(11) DEFAULT NULL,PRIMARY KEY (`id`),KEY `paciente_id` (`paciente_id`),CONSTRAINT `valoracion_paciente_ibfk_1` FOREIGN KEY (`paciente_id`) REFERENCES`pacientes` (`id`));
